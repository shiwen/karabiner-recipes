{
    "title": "Map home and end to control+a, control+e",
    "rules": [
        {
            "description": "Map home and end to control+a, control+e",
            "manipulators": [
                {
                    "from": {
                        "key_code": "home",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "a",
                            "modifiers": [
                                "control"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "end",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": [
                                "control"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
